{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["src/**/*", "libs/**/*", "global.d.ts"],
  "exclude": ["node_modules/*", "public/*"],
  "compilerOptions": {
    "importsNotUsedAsValues": "remove",
    "types": ["svelte", "jest"],
    "target": "ES2017" // "ES3" (default), "ES5", "ES6"/"ES2015", "ES2016", "ES2017" or "ESNext".
  }
}
