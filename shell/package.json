  "scripts": {
    "clean:all": "npx shx rm -rf public/bundle.* && npx shx rm -rf node_modules",
    "clean": "npx shx rm -rf public/bundle.*",
    "dev": "DEPLOY_ENVIRONMENT=local webpack-dev-server --https --config webpack.config.js --mode=development",
    "local": "DEPLOY_ENVIRONMENT=ultralocal webpack-dev-server --config webpack.config.js --mode=development",
    "dmc": "DEPLOY_ENVIRONMENT=thorsten webpack-dev-server --config webpack.config.js --mode=development",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "npm run clean && npm i && npm run build && fission app publish --verbose",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/shared/css/tailwind.css -o public/bundle.css -w"
  },
